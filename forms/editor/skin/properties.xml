	<xul>
		<javascript>
			<field name="mPreviewButton">null</field>

			<constructor><![CDATA[
				this.mPreviewButton = this.addActionButton('&modules.skin.bo.actions.Preview;', wToolkit.getIcon('eye', 'small'));
				var me = this;
				this.mPreviewButton.addEventListener('command', function(event) {me.onPreview();}, true);
				this.documentEditor.module.initPreview();
			]]></constructor>


			<method name="onPreview">
				<parameter name="event" />
				<body><![CDATA[
					this.documentEditor.executeServerAction('properties', 'RefreshPreview');
				]]></body>
			</method>

			<method name="getRefreshPreviewParameters">
				<body><![CDATA[
					var documentInfos = this.documentEditor.documentInfos;

					var attributes = {
						action: 'GetMd5',
						cmpref: this.mDocumentId,
						lang : this.mLang
					};

					var fields = this.fields;
					var field = null;
					for (var name in fields)
					{
						field = fields[name];
						if (('modified' in field) && field.modified)
						{
							attributes[name] = field.value;
						}
					}
					return attributes;
				]]></body>
			</method>

			<method name="fireRefreshPreviewComplete">
				<parameter name="result" />
				<body><![CDATA[
					if (result.status == "OK")
					{
						var md5 = result.contents.md5;
						wCore.debug(wCore.dump(result.contents));
						this.documentEditor.module.showPreview(md5);
					}
					else
					{
						this.showErrorMessage(result.contents.errorMessage);
					}
					return null;
				]]></body>
			</method>
		</javascript>
	</xul>
