			<staticitem label="&amp;modules.skin.list.borderstyle.Double;" value="double"/>
			<staticitem label="&amp;modules.skin.list.borderstyle.Groove;" value="groove"/>
			<staticitem label="&amp;modules.skin.list.borderstyle.Inset;" value="inset"/>
			<staticitem label="&amp;modules.skin.list.borderstyle.Outset;" value="outset"/>
