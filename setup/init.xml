			label="Répétition pour Habillage" description="Répétition pour Habillage">
			<staticitem label="&amp;modules.skin.list.brepetition.Repeat;" value="repeat"/>
			<staticitem label="&amp;modules.skin.list.brepetition.Repeatx;" value="repeat-x"/>
			<staticitem label="&amp;modules.skin.list.brepetition.Repeaty;" value="repeat-y"/>
			<staticitem label="&amp;modules.skin.list.brepetition.Norepeat;" value="no-repeat"/>
		</staticlist>

		<staticlist listid="modules_skin/fontfamily"
			label="Police de caractères pour Habillage" description="Police de caractères pour Habillage">
			<staticitem label="&amp;modules.skin.list.fontfamily.Serif;" value="'Times New Roman', Times, serif"/>
			<staticitem label="&amp;modules.skin.list.fontfamily.Serif2;" value="Georgia, serif"/>
			<staticitem label="&amp;modules.skin.list.fontfamily.Serif3;" value="Garamond, serif"/>
			<staticitem label="&amp;modules.skin.list.fontfamily.Sansserif;" value="Arial, Helvetica, sans-serif"/>
