			label="Style de soulignement des liens pour Habillage" description="Style de soulignement des liens pour Habillage">
			<staticitem label="&amp;modules.skin.list.textdecoration.None;" value="none"/>
			<staticitem label="&amp;modules.skin.list.textdecoration.Underline;" value="underline"/>
			<staticitem label="&amp;modules.skin.list.textdecoration.Overline;" value="overline"/>
			<staticitem label="&amp;modules.skin.list.textdecoration.Strike;" value="line-through"/>
